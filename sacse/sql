/*INSERT QUERY*/

INSERT INTO 'post'('title','content') VALUES ('hi','cse');


/*UPDATE QUERY*/

UPDATE `post` SET `TITLE`='HELLO',`CONTENT`='CSE' WHERE ID=11

/*DELETE QUERY*/

DELETE FROM 'POST' WHERE 'id' =2

/*SELECT ALL QUERY*/

SELECT * FROM 'POST'

/*SELECT WITH CONDITION QUERY*/

SELECT*FROM 'POST'WHERE 'ID'=10